{
   "manifest_version": 2,
   "name": "Douyu Barrage",
   "description": "Send Douyu Barrage",
   "version": "0.1.1",
   "icons": {
      "128": "icons/tropical_fish_117.png",
      "32": "icons/tropical_fish_39.png"
   },
   "incognito": "split",
   "permissions": [ "tabs", "\u003Call_urls>" ],
   "content_scripts": [
       {
         "matches": ["http://*.douyu.com/*", "https://*.douyu.com/*"],
         "js": ["src/js/libs/jquery-2.1.4.min.js", "src/js/content.js"]
       }
     ]
}
